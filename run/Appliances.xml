            <ModbusRegisterRead address="0x000C" type="InputFloat">
                <ModbusRegisterReadValue name="Power"/>
            </ModbusRegisterRead>
            <ModbusRegisterRead address="0x0156" type="InputFloat">
                <ModbusRegisterReadValue name="Energy"/>
            </ModbusRegisterRead>
        </ModbusElectricityMeter -->
    </Appliance>
